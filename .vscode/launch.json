{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Gulp",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": false,
            "args": ["pull"],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "outFiles": [
                "${workspaceRoot}/dist/*.js"
            ],
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "sourceMaps": true
        }
    ]
}